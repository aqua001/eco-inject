{
	"manifest_version": 2,
	"name": "ECO页面脚本拦截注入工具",
	"version": "1.0.0",
	"icons": {
        "16": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
	"browser_action": {
		"default_icon": "images/logo.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"downloads", 
		"webRequest", 
		"webRequestBlocking",
		"notifications",
		"*://*/*"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"web_accessible_resources": [
		"returnjs.js",
		"images/*"
	]
}